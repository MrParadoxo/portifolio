			<button left="200" top="55" width="30" height="150" name="AtrBut" text=">>">
				<event name="onClick">
					local pop = self:findControlByName("popAtributo");
				
					if pop ~= nil then
						pop:setNodeObject(self.sheet);
						pop:showPopupEx("rightCenter", self.AtrBut);
