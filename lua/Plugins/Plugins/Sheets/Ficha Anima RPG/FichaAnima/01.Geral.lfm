		</popup>
		<popup name="popCA" width="600" height="210" backOpacity="0.4" autoScopeNode="false">
			<flowLayout align="top" autoHeight="true" margins="{bottom=4}">
				<Protection nome="Armadura" field="armaduraCa"/>
