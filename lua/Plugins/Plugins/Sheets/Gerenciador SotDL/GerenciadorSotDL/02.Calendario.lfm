<?xml version="1.0" encoding="UTF-8"?>
<form name="frmGerenciador02" align="client" theme="dark" margins="{top=1}">
	<script> 
		<![CDATA[
		local function write(str)
            local mesa = Firecast.getMesaDe(sheet);
            if str then
                mesa.activeChat:escrever(str);
            else
                mesa.activeChat:escrever("String nula");
            end;
        end;
		]]>	
	</script>
    <scrollBox align="client">
    	<template name="shortTitleCenter">
			<flowPart minWidth="20" maxWidth="50" height="15">
				<label align="top" class="tituloCampo" fontSize="10" text="$(text)" horzTextAlign="center" wordWrap="true" textTrimming="none" autoSize="true"/>
			</flowPart>
		</template>
    	<template name="smallTitleCenter">
			<flowPart minWidth="70" maxWidth="100" height="15">
				<label align="top" class="tituloCampo" fontSize="10" text="$(text)" horzTextAlign="center" wordWrap="true" textTrimming="none" autoSize="true" hint="$(hint)"/>
			</flowPart>
		</template>
		<template name="longTitleCenter">
			<flowPart minWidth="100" maxWidth="150" height="15">
				<label align="top" class="tituloCampo" fontSize="10" text="$(text)" horzTextAlign="center" wordWrap="true" textTrimming="none" autoSize="true"/>
			</flowPart>
		</template>
		<template name="longComboBox">
			<flowPart minWidth="100" maxWidth="150" height="20">
				<comboBox align="top" fontSize="10" field="$(field)" items="$(items)" horzTextAlign="center"/>
			</flowPart>
		</template>
		<template name="shortEdit">
			<flowPart minWidth="20" maxWidth="50" height="20">
				<edit align="client" fontSize="10" field="$(field)" vertTextAlign="center"/>
			</flowPart>
		</template>
		<template name="shortNumEdit">
			<flowPart minWidth="20" maxWidth="50" height="20">
				<edit align="client" fontSize="10" field="$(field)" vertTextAlign="center" type="number" min="$(min)"/>
			</flowPart>
		</template>
		<template name="longEdit">
			<flowPart minWidth="100" maxWidth="150" height="20">
				<edit align="client" fontSize="10" field="$(field)" vertTextAlign="center"/>
			</flowPart>
		</template>

		<popup name="popClima" width="240" height="210" backOpacity="0.4" autoScopeNode="false">
