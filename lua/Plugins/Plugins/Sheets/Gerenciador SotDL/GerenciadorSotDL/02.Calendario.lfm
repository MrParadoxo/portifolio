		<layout align="top" height="345">
    		<layout align="left" width="250" margins="{right=5}">
    			<rectangle align="client" color="Black"/>

    			<label width="250" height="20" text="MESES" horzTextAlign="center"/>

    			<layout left="0" top="25" width="250" height="25">
	    			<button left="10" top="0" width="25" height="25" text="+" hint="Novo">
	    				<event name="onClick">
							if sheet~=nil then
								local meses = ndb.getChildNodes(sheet.listaMeses);
								local num = #meses + 1;

								local node = self.rclMeses:append();
								if node~=nil then
									node.ordem = num;
									node.mes = "Mês " .. num;
									node.dias = 30;
								end;

								self.rclMeses:sort();
							end;
						</event>
	    			</button>
	    			<label left="115" top="0" width="40" height="25" text="Total" hitTest="true" hint="Quantos dias dura o ano."/>
	    			<rectangle left="165" top="0" width="35" height="25" color="black" strokeColor="white" strokeSize="1"/>
					<label left="165" top="0" width="35" height="25" field="dias" horzTextAlign="center" fontSize="12" name="dias" hitTest="true" hint="Se estiver vermelho a duração do ano e das estações está dessincronizada. "/>
				</layout>

				<label left="6" top="50" width="30" height="25" text="Nº" horzTextAlign="center" hitTest="true" hint="Qual a ordem dos meses?"/>
				<label left="45" top="50" width="110" height="25" text="Nome" horzTextAlign="center"/>
				<label left="155" top="50" width="55" height="25" text="Duração" horzTextAlign="center" hitTest="true" hint="Quantos dias dura esse mês."/>

    			<recordList left="5" top="80" width="240" height="260" name="rclMeses" field="listaMeses" templateForm="frmGerenciador02_MES" layout="vertical">
					<event name="onCompare"><![CDATA[               
		                if (tonumber(nodeA.ordem) or 0) < (tonumber(nodeB.ordem) or 0) then
		                    return -1;
		                elseif (tonumber(nodeA.ordem) or 0) > (tonumber(nodeB.ordem) or 0) then
		                    return 1;
		                else   
		                    return 0;
		                end;               
		                ]]>
					</event>
				</recordList>

				<dataLink fields="{'duracaoEstacao', 'dias'}">
					<event name="onChange">
						if sheet~= nil then
							if sheet.dias ~= sheet.duracaoEstacao then
								self.dias.fontColor = "red";
							else
								self.dias.fontColor = "white";
							end;
						end;
					</event>
				</dataLink>
    		</layout>

    		<layout align="left" width="250" margins="{right=5}">
    			<rectangle align="client" color="Black"/>

    			<label width="250" height="20" text="DIAS DA SEMANA" horzTextAlign="center"/>

    			<layout left="0" top="25" width="250" height="25">
	    			<button left="10" top="0" width="25" height="25" text="+" hint="Novo">
	    				<event name="onClick">
							if sheet~=nil then
								local semana = ndb.getChildNodes(sheet.listaSemana);
