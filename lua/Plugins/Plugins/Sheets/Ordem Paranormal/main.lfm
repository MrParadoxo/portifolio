                                       onClick="NDB.deleteNode(self.boxDetalhesDoItem2.node);" />

                        </layout>                       

                        <layout align="top" top="15" height="130">              
                                <image align="left" editable="true" field="campoURLImagem2" width="130"/>                               

                                <layout align="client" margins="{left=2}">
                                        <edit align="top" field="campoSubTitulo2" height="30"/>
                                        <textEditor align="top" field="campoTextoGrande2" height="100"/>
                                </layout>                      
                        </layout>
                </rectangle>
        </dataScopeBox>

               
