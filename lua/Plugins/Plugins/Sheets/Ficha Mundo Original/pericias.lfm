<?xml version="1.0" encoding="UTF-8"?>
<form name="pericia" height="100" width="700">
<rectangle align="client">	
<scrollBox align="client">
<flowLayout align="client" height="500" autoHeight="true" horzAlign="center" lineSpacing="3">	
		
		<label text="Nome da Pericia" width="170" height="30" left="30" fontColor="black"/>
		<checkBox height="30" top="35"/>
