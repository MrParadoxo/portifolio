		<label align="left" width="40" field="base6" horzTextAlign="center" margins="{left=5}"/>
		<label align="left" width="40" field="base7" horzTextAlign="center" margins="{left=5}"/>
		<label align="left" width="40" field="base8" horzTextAlign="center" margins="{left=5}"/>
		<label align="left" width="40" field="base9" horzTextAlign="center" margins="{left=5,right=5}"/>
	</layout>
	<layout align="top" height="25">
		<label align="left" width="75" text="Conhecidas" horzTextAlign="center" margins="{left=5}"/>
		<label align="left" width="40" field="conhecidas0" horzTextAlign="center" margins="{left=5}"/>
		<label align="left" width="40" field="conhecidas1" horzTextAlign="center" margins="{left=5}"/>
