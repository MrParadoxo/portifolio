		<label align="left" width="40" field="conhecidas6" horzTextAlign="center" margins="{left=5}"/>
		<label align="left" width="40" field="conhecidas7" horzTextAlign="center" margins="{left=5}"/>
		<label align="left" width="40" field="conhecidas8" horzTextAlign="center" margins="{left=5}"/>
		<label align="left" width="40" field="conhecidas9" horzTextAlign="center" margins="{left=5,right=5}"/>
