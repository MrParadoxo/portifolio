<?xml version="1.0" encoding="UTF-8"?>
<form name="frmGeneratorMagia" height="75" width="600" margins="{top=2}">
	<layout align="top" height="25">
		<comboBox align="left" width="200" fontColor="white" field="tipoMagia" items="{'Lista de Magias', 'Signos', 'Essencias'}" values="{'3', '2', '4'}" margins="{left=5}"/>/>
		<comboBox align="left" width="50" fontColor="white" field="atributoCD" items="{'FOR', 'DES', 'CON', 'INT', 'SAB', 'CAR', '-'}" values="{'1', '2', '3', '4', '5', '6', '7'}" margins="{left=5}"/>/>
		<comboBox align="left" width="50" fontColor="white" field="atributoBonus" items="{'FOR', 'DES', 'CON', 'INT', 'SAB', 'CAR', '-'}" values="{'1', '2', '3', '4', '5', '6', '7'}" margins="{left=5}"/>/>
	</layout>
	<layout align="top" height="25">
		<label align="left" width="75" text="Diarias" horzTextAlign="center" margins="{left=5}"/>
