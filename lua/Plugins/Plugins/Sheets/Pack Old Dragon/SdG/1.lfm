	--Campos calculados
	<dataLink field="Des">
                <event name="onChange">
                        local mod = math.floor(((sheet.Des or 0) - 10) / 2);
                     

                        if (mod >= 0) then
                                mod = "+" .. mod;
                        end;                       

                        sheet.CADest = mod;
                </event>
        </dataLink>
		
	<dataLink fields="{'CADest', 'CAArmadura', 'CaEsc', 'CAItem', 'CAOutro1', 'CAOutro2'}">
	<event name="onChange">
               sheet.CA = ((tonumber(sheet.CADest) or 0) + 10) +
                              (tonumber(sheet.CaEsc) or 0) +
							  (tonumber(sheet.CAArmadura or 0) or 0) +
                              (tonumber(sheet.CAItem) or 0) +
							  (tonumber(sheet.CAOutro1) or 0) +
                              (tonumber((sheet.CAOutro2) or 0));                  

               if sheet.CA >= 0 then
                       sheet.CA = sheet.CA;
               end;
        </event>
        </dataLink>
		
		<dataLink fields="{'JP', 'Des'}">
             <event name="onChange">
               sheet.JPDes = (tonumber(sheet.JP or 0)) -
                             (math.floor(((sheet.Des or 0) - 10) / 2));                  

               if sheet.JPDes >= 0 then
                       sheet.JPDes = sheet.JPDes;
               end;
        </event>
     </dataLink>
	 
	 <dataLink fields="{'JP', 'Con'}">
             <event name="onChange">
               sheet.JPCon = (tonumber(sheet.JP or 0)) -
                             (math.floor(((sheet.Con or 0) - 10) / 2));                  

               if sheet.JPCon >= 0 then
                       sheet.JPCon = sheet.JPCon;
               end;
        </event>
     </dataLink>
	 
	  <dataLink fields="{'JP', 'Sab'}">
             <event name="onChange">
               sheet.JPSab = (tonumber(sheet.JP or 0)) -
                             (math.floor(((sheet.Sab or 0) - 10) / 2));                  

               if sheet.JPSab >= 0 then
                       sheet.JPSab = sheet.JPSab;
               end;
        </event>
     </dataLink>
		
	<dataLink field="Des">
                <event name="onChange">
                        local mod = math.floor(((sheet.Des or 0) - 10) / 2);
                     

                        if (mod >= 0) then
                                mod = "+" .. mod;
                        end;                       

                        sheet.BADes = mod;
                </event>
        </dataLink>
		
			<dataLink field="For">
                <event name="onChange">
                        local mod = math.floor(((sheet.For or 0) - 10) / 2);
                     

                        if (mod >= 0) then
                                mod = "+" .. mod;
                        end;                       

                        sheet.BAFor = mod;
                </event>
        </dataLink>  
